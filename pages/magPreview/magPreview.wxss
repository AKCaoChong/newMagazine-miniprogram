.container {
  padding-bottom: 0;
}
.container .bg-swiper{
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.container .line-group{
  margin: 20rpx 0;
  width: 100%;
  height: 3rpx;
  background-color: #cccccc;
}

.line-group .line-xi{
  
  width: 100%;
  height: 3rpx;
  background-color: #cccccc;
}

.line-group .line-cu{
  width: 30%;
  height: 12rpx;
  background-color: #333333;
  /* float: left; */
  position: relative;
  top: -6rpx;
  left: 20rpx;
  border-radius: 6rpx;
  box-shadow: 0 5rpx 30rpx #cccccc;
}

.container .read{
  width: 50%;
  height: 80rpx;
  background-color: #C52E43;
  color: #ffffff;
  line-height: 80rpx;
  text-align: center;
  border-radius: 20rpx;
  position: fixed;
  bottom: 50px;
}

.container .android-read{
  width: 100%;
  height: 200rpx;
  /* background-color: #ffffff; */
  position: fixed;
  bottom: 50px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  
}
.android-read .android{
  width: 48%;
  height: 80rpx;
  font-size: 30rpx;
  color: white;
  font-weight: 500;
  text-align: center;
  line-height: 80rpx;
  border-radius: 20rpx;
}

.android-read .code{
  background-color: #333333;
 
}

.android-read .buy{
  background-color: #C52E43;
  
}


.container .func{
  background-color: rgba(0, 0, 0, .9);
  /* opacity: 0.2; */
  position: fixed;
  right: -140rpx;
  width: 140rpx;
  height: 80rpx;
  border-top-left-radius: 20rpx;
  border-bottom-left-radius: 20rpx;
  z-index: 1000;
}

.container .collect{
  top: 300rpx;
}

.container .share{
  top: 400rpx;
}

