<view class="container">
  <view class="user-info">
    <image class="header" src="{{user.head}}"></image>
    <text class="nick-name">{{user.name}}</text>
  </view>
  <view class="mag-list">
    <block wx:for="{{magazineList}}" wx:key="id">
      <mag-hor bindreadCodeTap="readCodeTap" mag="{{item}}" isMy="true"></mag-hor>
    </block>
    
  </view>
</view>
